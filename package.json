  "scripts": {
    "cli": "node core/scripts/cli.js",
    "watch-test": "npm test -- --watch",
    "install_types": "npx types-installer install -p yarn -D",
    "build": "npx rimraf dist && npx webpack --config ./config/targets.config.js",
    "test": "npx cross-env NODE_ENV=test webpack --config ./config/tests.config.js",
    "migrate": "npx cross-env NODE_ENV=test npx sequelize db:migrate && npx cross-env NODE_ENV=development npx sequelize db:migrate",
    "undo": "npx cross-env NODE_ENV=test npx sequelize db:migrate:undo && npx cross-env NODE_ENV=development npx sequelize db:migrate:undo",
    "start": "npx rimraf dist && npx cross-env NODE_ENV=development npx webpack --config ./config/targets.config.js --watch --hide-modules",
    "production": "git pull && yarn && npx cross-env NODE_ENV=production NODE_NDEBUG=1 npx sequelize db:migrate && npx rimraf dist && npx cross-env NODE_ENV=production npx parallel-webpack --config ./config/targets.config.js && npm rebuild node-sass && pm2 stop all && pm2 start dist/server.js --node-args='--expose-gc'",
    "check_production": "npx cross-env NODE_ENV=production npx parallel-webpack --config ./config/targets.config.js; node dist/server.js",
    "analyze": "npx cross-env NODE_ENV=production npx parallel-webpack --config ./config/targets.config.js --watch"
  },
